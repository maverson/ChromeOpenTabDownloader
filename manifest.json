{
    "name": "ChromeOpenTabDownloader",
    "description": "Download images open in tabs",
    "version": "0.0.1",
    "permissions": [
        "tabs",
        "downloads",
        "unlimitedStorage",
        "tabGroups"
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 2
  }